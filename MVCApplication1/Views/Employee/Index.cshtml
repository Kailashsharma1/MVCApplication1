@model IEnumerable<MVCApplication1.Models.Employee>
@{
}
<a asp-action="Create" asp-controller="Employee" class="btn"> Create </a><br />

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Salary</th>
            <th>Phone</th>
            <th>Action</th>
        </tr>
    </thead>
    @foreach(var emp in Model)
    {
        <tr>
            <td>@emp.Id</td>
            <td>@emp.Name</td>
            <td>@emp.Age</td>
            <td>@emp.Salary</td>
            <td>@emp.Phone</td>
            <td>
                <div></div>
            <a asp-action="Edit" asp-controller="Employee" asp-route-Id="@emp.Id" class="btn">Edit</a>
            <a asp-action="Delete" asp-controller="Employee" asp-route-Id="@emp.Id" class="btn" id="deletebtn">Delete</a>
            </td>
            

        </tr>
    }
</table>
